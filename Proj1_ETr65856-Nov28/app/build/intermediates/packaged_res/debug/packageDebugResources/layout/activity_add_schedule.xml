<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/jokeImageView"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AddSchedule"
    tools:layout_editor_absoluteX="-22dp"
    tools:layout_editor_absoluteY="-2dp">

    <ImageButton
        android:id="@+id/btnHome"
        android:layout_width="49dp"
        android:layout_height="54dp"
        android:contentDescription="@string/descriptionHome"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.013"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.001"
        app:srcCompat="@drawable/home" />

    <TextView
        android:id="@+id/addSTv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/addSTv"
        android:textAlignment="center"
        android:textSize="38sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.086" />

    <Button
        android:id="@+id/addBtnS"
        android:layout_width="156dp"
        android:layout_height="63dp"
        android:backgroundTint="#3F51B5"
        android:text="@string/addBt"
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.875" />

    <TextView
        android:id="@+id/addTimeTv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/addTimeTv"
        android:textAlignment="textEnd"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.27"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.541" />

    <TextView
        android:id="@+id/addFNameTvS"
        android:layout_width="wrap_content"
